{
	"hosting": {
		"public": "public",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"redirects": [{
			"source": "/privacy-policy",
			"destination": "https://www.oya.sg/privacy-policy",
			"type": 301
		}, {
			"source": "/terms-and-conditions",
			"destination": "https://www.oya.sg/terms-conditions",
			"type": 301
		}, {
			"source": "/esim-instructions-video",
			"destination": "https://firebasestorage.googleapis.com/v0/b/pi-redirector.appspot.com/o/public%2FeSim%20OYA%20Instruction%20Video%20v4%20-%20Apple%20API.mp4?alt=media&token=1f84331e-6e6d-4766-a225-9af2cbda0bba",
			"type": 301
		}, {
			"source": "/fully-digital",
			"destination": "https://support.oya.sg/support/solutions/articles/75000016846-what-is-oya-",
			"type": 301
		}, {
			"source": "/minimum-age",
			"destination": "https://support.oya.sg/solution/articles/75000017098-what-is-the-minimum-age-requirement-for-signing-up-",
			"type": 301
		}, {
			"source": "/location-required",
			"destination": "https://support.oya.sg/solution/articles/75000017100-why-does-my-location-need-to-accessed-during-registration-",
			"type": 301
		}, {
			"source": "/about-membership",
			"destination": "https://support.oya.sg/solution/articles/75000017139-why-become-an-oya-member-",
			"type": 301
		}, {
			"source": "/current-pricing",
			"destination": "https://support.oya.sg/solution/articles/75000017142-what-are-the-price-plans-",
			"type": 301
		}]
	}
}
